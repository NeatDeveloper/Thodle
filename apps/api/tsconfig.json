{
    "extends": "@repo/types/tsconfig.json",
    "compilerOptions": {
        "paths": {
            "helpers": [
                "./helpers"
            ],
            "helpers/*": [
                "./helpers/*"
            ],
            "middlewares": [
                "./middlewares"
            ],
