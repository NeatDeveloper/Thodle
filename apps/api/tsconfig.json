{
    "extends": "../../tsconfig.json",
    "compilerOptions": {
        "paths": {
            "helpers": [
                "./src/helpers"
            ],
            "helpers/*": [
                "./src/helpers/*"
            ],
            "middlewares": [
                "./src/middlewares"
            ],
            "middlewares/*": [
                "./src/middlewares/*"
            ],
            "routes": [
                "./src/routes"
            ],
            "routes/*": [
                "./src/routes/*"
            ],
            "lib": [
                "./lib"
            ],
            "lib/*": [
                "./lib/*"
            ]
        }
    }
}
