{
    "extends": "@repo/types/tsconfig.json",
    "compilerOptions": {
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "paths": {
            "configuration": [
                "./src/configuration"
            ],
            "configuration/*": [
                "./src/configuration/*"
            ],
            "commands": [
                "./src/commands"
            ],
            "commands/*": [
                "./src/commands/*"
            ],
            "middlewares": [
                "./src/middlewares"
            ],
            "middlewares/*": [
                "./src/middlewares/*"
            ],
            "helpers": [
                "./src/helpers"
            ],
            "helpers/*": [
                "./src/helpers/*"
            ],
            "src": [
                "./src"
            ],
            "src/*": [
                "./src/*"
            ],
        }
    }
}
