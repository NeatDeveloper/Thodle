model Student {
    id String @id

    group Int?

    Amplua Amplua @relation(fields: [id], references: [id], onDelete: Cascade)
    Group  Group? @relation(fields: [group], references: [id])

    createdAt DateTime @default(now()) @map("created_at")
    updatedAt DateTime @updatedAt @map("updated_at")

    @@index([group])
    @@schema("amplua")
}
