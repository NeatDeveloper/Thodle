model Tutor {
    id String @id

    group Int? @unique

    Group  Group? @relation(fields: [group], references: [id])
    Amplua Amplua @relation(fields: [id], references: [id], onDelete: Cascade)

    updatedAt DateTime @updatedAt @map("updated_at")
    createdAt DateTime @default(now()) @map("created_at")

    @@index([group])
    @@map("Tutors")
    @@schema("amplua")
}
