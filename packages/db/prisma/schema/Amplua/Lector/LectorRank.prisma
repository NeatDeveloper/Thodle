/// Модель **Ранги Преподавателей**
///
/// Предназначен для хранения рангов преподавателей.
/// Пример: `Доцент(доц.)`, `Профессор(проф.)` и тд
///
/// @namespace Amplua
/// @erd Amplua
model LectorRank {
    /// Уникальный идентификатор
    id Int @id @default(autoincrement())

    /// Название
    name  String @unique

    /// Короткий вариант названия
    short String @unique

    lectors Lector[]

    @@map("LectorRanks")
    @@schema("amplua")
}
